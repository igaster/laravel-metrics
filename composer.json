{
    "name": "igaster/laravel-metrics",
    "description": "Description",
    "keywords": ["laravel", "package"],
    "license": "MIT",
    "homepage": "https://github.com/igaster/laravel-metrics.git",
    "authors": [
        {
            "name": "Giannis Gasteratos",
            "email": "igasteratos@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": ">=5.6"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4" : {
            "Igaster\\LaravelMetrics\\"        : "src/",
            "Igaster\\LaravelMetrics\\Tests\\" : "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Igaster\\LaravelMetrics\\PackageServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
